{
    "name": "github-project-context-generator",
    "version": "1.0.0",
    "description": "Generate comprehensive context of a GitHub project by cloning and merging selected files",
    "main": "src/index.js",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "server": "node server.js",
        "start:frontend": "cd frontend && npm start",
        "start:backend": "cd backend && npm start",
        "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
    },
    "keywords": [
        "github",
        "project",
        "context",
        "generator",
        "llm"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "minimatch": "^3.0.4",
        "minimist": "^1.2.5"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}
